@model IList<Productgold.Models.Category>       
<div class="category leftbar">
    <h3 class="title">Danh mục
    </h3>
    <ul>
      
        @for (int i = 0; i < Model.Count; i++)
        {
            <li>
                <a href="/Sanpham/Nhom/@Model[i].keys" title="@Model[i].name">
                    @Model[i].name
                </a>
            </li> 
        }

    </ul>
</div>
