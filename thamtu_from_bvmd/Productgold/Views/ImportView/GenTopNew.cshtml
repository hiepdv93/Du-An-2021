@model IList<Productgold.Models.News>       
<div class="category leftbar">
    <h3 class="title">Tin tức
    </h3>
    <ul>
      
        @for (int i = 0; i < Model.Count; i++)
        {
            <li>
                <a href="/Tintuc/Chitiet/@Model[i].new_key" title="@Model[i].title">
                    @Model[i].title
                </a>
            </li> 
        }

    </ul>
</div>
