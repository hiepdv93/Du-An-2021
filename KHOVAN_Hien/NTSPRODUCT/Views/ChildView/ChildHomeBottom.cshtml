@{
    var chooseWe = (List<NTSPRODUCT.Models.WhyChooseUss>)ViewBag.chooseWe;
    var sayWes = (List<NTSPRODUCT.Models.SayWe>)ViewBag.sayWes;
    var partner = (List<NTSPRODUCT.Models.Partner>)ViewBag.partner;
    var tags = (List<NTSPRODUCT.Models.Tagpro>)ViewBag.tags;
    var conf = (NTSPRODUCT.Models.Config)ViewBag.conf;
}
<section id="faedback-home">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
                <div class="titel-home">
                    <h2 class="uppercase text-center">@ClassExten.GetLangKey("key_ykkh")</h2>
                </div>
                <div id="owl-faedback-home" class="owl-carousel owl-theme">
                    @foreach (var item in sayWes)
                    {
                        <div class="item">
                            <div class="content_danhgia">
                                <p>@item.contents</p>
                                <span><span>@item.fullName</span> - @item.numberOder</span>
                            </div>
                            <div class="img_danhgia">
                                <span>
                                    <img src="@item.avata.Replace("uploads", "uploads/_thumbs")" alt="@item.fullName">
                                </span>
                            </div>
                        </div>
                    }
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-5 col-lg-5">
                <div class="titel-home">
                    <h2 class="uppercase text-center">@ClassExten.GetLangKey("key_videoHome")</h2>
                </div>
                <div class="video-home">
                    @Html.Raw(conf.homeVideo)
                </div>
            </div>
        </div>
    </div>
</section>
<section id="commitment-home">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="box-commitment-home">
                    @Html.Raw(conf.commitment)
                </div>
            </div>
        </div>
    </div>
</section>
<section id="why-choose-us-home">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="titel-home">
                    <h2 class="uppercase text-center">@ClassExten.GetLangKey("key_visao")</h2>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                <div class="why-chose">
                    <div class="row">
                        @foreach (var item in chooseWe)
                        {
                            <div class="col col-xs-12 col-sm-12 col-md-6 col-lg-6">
                                <div class="col-inner">
                                    <div class="box has-hover has-hover box-vertical box-text-bottom">
                                        <div class="box-image">
                                            <div class="box-icon">
                                                <img src="@item.image.Replace("uploads", "uploads/_thumbs")" alt="@item.title">
                                            </div>
                                        </div>
                                        <div class="box-text text-left">
                                            <div class="box-text-inner">
                                                <h3 class="uppercase">@item.title</h3>
                                                <p>@item.contents</p>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                <div class="contact-home">
                    <div class="title-portfolio">
                        <h3 class="font-taviraj">@ClassExten.GetLangKey("key_lhtv")</h3>
                    </div>
                    <div class="form-contact">
                        <form action="" method="post" class="img-responsive">
                            <div class="card-body p-3">
                                <!--Body-->
                                <div class="form-group col-lg-12 col-sm-12">
                                    <div class="input-group mb-2">
                                        <div class="input-group-prepend">
                                            <div class="input-group-texts"><i class="fa fa-user"></i></div>
                                        </div>
                                        <input type="text" class="form-control" id="ctName" name="nombre" placeholder="@ClassExten.GetLangKey("key_hoten") (*)">
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-sm-12">
                                    <div class="input-group mb-2">
                                        <div class="input-group-prepend">
                                            <div class="input-group-texts"><i class="fa fa-envelope"></i></div>
                                        </div>
                                        <input type="email" class="form-control" id="ctEmail" name="email" placeholder="Email...">
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-sm-12">
                                    <div class="input-group mb-2">
                                        <div class="input-group-prepend">
                                            <div class="input-group-texts"><i class="fa fa-phone"></i></div>
                                        </div>
                                        <input type="text" class="form-control" id="ctPhone" name="nombre" placeholder="@ClassExten.GetLangKey("key_sdt") (*)">
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-sm-12">
                                    <div class="input-group mb-2">
                                        <div class="input-group-prepend">
                                            <div class="input-group-texts"><i class="fa fa-home"></i></div>
                                        </div>
                                        <input type="text" class="form-control" id="ctAddress" name="nombre" placeholder="@ClassExten.GetLangKey("key_diachi") ..">
                                    </div>
                                </div>
                                <div class="form-group col-lg-12 col-sm-12">
                                    <div class="input-group mb-2">
                                        <div class="input-group-prepend">
                                            <div class="input-group-texts"><i class="fa fa-pencil"></i></div>
                                        </div>
                                        <textarea class="form-control" id="ctNote" rows="3" placeholder="@ClassExten.GetLangKey("key_ndmota") (*)"></textarea>
                                    </div>
                                </div>
                                <div class="text-right col-lg-12 col-sm-12">
                                    <div class="btn">
                                        <button onclick="SendContact()" type="button" class="submit"> @ClassExten.GetLangKey("key_guilienhe")<i class="fa fa-send"></i></button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section id="tags-home">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="box-tags-home">
                    <div class="tags-name">Tags: </div>
                    @foreach (var item in tags)
                    {
                        <a class="tags-item" href="/tags/@item.tagKey" title="@item.tagName">@item.tagName</a>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
<section id="partner-home">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="titel-home partner">
                    <h2 class="uppercase text-center">@ClassExten.GetLangKey("key_doitac")</h2>
                </div>
                <div id="owl-partner-home" class="owl-carousel owl-theme">
                    @foreach (var item in partner)
                    {
                        <div class="item">
                            <div class="col-inner">
                                <div class="box has-hover gallery-box box-none">
                                    <div class="box-image">
                                        <img src="@item.pImage.Replace("uploads", "uploads/_thumbs")" class="attachment-medium size-medium" alt="@item.pName">
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>

