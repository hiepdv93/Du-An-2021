@model NTSPRODUCT.Models.Config
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="breadcrumb_background">
    <span class="overlay"></span>
    <div class="title_full">
        <div class="container a-center">
            <h1 class="title_page">@ClassExten.GetLangKey("key_lienhe")</h1>
        </div>
    </div>
</div>
<section class="bread-crumb">
    <span class="crumb-border"></span>
    <div class="container">
        <div class="rows">
            <div class="col-xs-12 a-left">
                <ul class="breadcrumb" itemscope="">
                    <li class="home">
                        <a itemprop="url" href="/" title="@ClassExten.GetLangKey("home")"><span itemprop="title">@ClassExten.GetLangKey("home")</span></a>
                        <span class="mr_lr">&nbsp;<i class="fa fa-caret-right"></i>&nbsp;</span>
                    </li>
                    <li><strong><span itemprop="title">@ClassExten.GetLangKey("key_lienhe")</span></strong></li>
                </ul>
            </div>
        </div>
    </div>
</section>
<section class="page-detail">
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-xs-12 col-lg-63">
                <div class="form-contact">
                    <div class="card-body p-3">
                        <!--Body-->
                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-user"></i></div>
                                </div>
                                <input type="hidden" id="referencesId" value="-1">
                                <input maxlength="100" type="text" class="form-control" id="rfullName" name="rfullName" placeholder="Họ tên (*)">
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-envelope"></i></div>
                                </div>
                                <input maxlength="100" type="email" class="form-control" id="rEmail" name="rEmail" placeholder="Email (*)">
                            </div>
                        </div>



                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-pencil"></i></div>
                                </div>
                                <input maxlength="100" type="password" class="form-control" id="rPass" name="rPass" placeholder="Mật khẩu (*)">
                            </div>
                        </div>

                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-phone"></i></div>
                                </div>
                                <input maxlength="100" type="password" class="form-control" id="rPassConfirm" name="rPassConfirm" placeholder="Nhập lại mật khẩu (*)">
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-pencil"></i></div>
                                </div>
                                <input maxlength="100" type="text" class="form-control" id="rCodeIntro" name="rCodeIntro" placeholder="Mã giới thiệu (*)">
                            </div>
                        </div>

                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-pencil"></i></div>
                                </div>
                                <input maxlength="100" type="text" class="form-control" id="rRegisterkey" name="rRegisterkey" placeholder="Mã bảo mật (*)">
                            </div>
                        </div>

                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-phone"></i></div>
                                </div>
                                <input maxlength="20" type="text" class="form-control" id="rPhone" name="rPhone" placeholder="Điện thoại ">
                            </div>
                        </div>
                        <div class="form-group col-lg-6 col-sm-6">
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <div class="input-group-texts"><i class="fa fa-home"></i></div>
                                </div>
                                <input maxlength="150" type="text" class="form-control" id="raddresss" name="raddresss" placeholder="Địa chỉ ..">
                            </div>
                        </div>
                        <div class="col-lg-6 col-sm-6" id="keyRegister">
                        </div>
                        <div class="text-right col-lg-6 col-sm-6">
                            <div class="btn">
                                <button onclick="SendRegister()" type="button" class="submit">Đăng ký <i class="fa fa-send"></i></button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 col-xs-12 col-lg-6">
                <div class="title-contact">
                    @Html.Raw(Model.conTact)
                </div>
            </div>
        </div>

    </div>
</section>
<script>
    $.post("/CustomerSite/RenderKeyReg", function (result) {
        $("#keyRegister").html(result);
    });
</script>