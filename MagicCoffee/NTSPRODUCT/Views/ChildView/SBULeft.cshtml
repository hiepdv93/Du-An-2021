@model IList<NTSPRODUCT.Models.GroupPartner>
<div id="leftPanel">
    <div class="module news-category clearfix">
        <div class="module-header">
            <h3><span>@ViewBag.groupName</span></h3>
        </div>
        <div class="catelogy-new">
            <ul class="nav0">
                @foreach (var item in Model)
                {
                    <li>
                        <a href="/@ViewBag.urlPro/@item.keys" title="@item.name" class="top-link "><span>@item.name</span></a>
                    </li>
                }
            </ul>
        </div>
    </div>
</div>


