<div id="modamSuport" class="modal fade" role="dialog" onscroll="scroll" data-backdrop="static" style="background-color: rgba(0,0,0,0.3)">
    <div class="modal-dialog w700">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header" style="background-color: #5db2ff; color: white">
                <button type="button" id="close_producet" class="close" data-dismiss="modal">&times;</button>
                <h4 class="titleSuport"></h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Tên Nhân Viên Hỗ Trợ<span class="text-red">(*)</span></label>
                                <input type="hidden" value="" id="idSuport" />
                                <input type="text" maxlength="100" class="form-control" name="advName" id="fullName" placeholder="Tên Nhân Viên.." required ="required">
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Loại</label>
                                <select class="form-control" id="sType" name="atype">
                                    <option value="1">Skype</option>
                                    <option value="3">Zalo</option>
                                    <option value="2">Hotline</option>
                                </select>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Nick<span class="text-red"></span></label>
                                <input type="text"  maxlength="200" class="form-control" name="advName" id="insnick" placeholder="Tên Nick.." required ="required">
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Số điện thoại</label>
                                <input type="text"  maxlength="25" class="form-control" name="advName" id="phone" placeholder="Số Điện Thoại.." required ="required">
                            </span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Địa Chỉ</label>
                                <input type="text"  maxlength="100" class="form-control" name="advName" id="addresss" placeholder="Địa chỉ.." required="required">
                            </span>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Email<span class="text-red"></span></label>
                                <input type="text"  maxlength="100" class="form-control" name="advName" id="email" placeholder="Email.." required="required">
                            </span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <span class="input-icon icon-right">
                                <label>Thứ tự</label><span class="text-red">(*)</span>
                                <input type="number" value="1" class="form-control" placeholder="Thứ tự" id="numberOder" name="numberOder" required="required">
                            </span>
                        </div>
                    </div>
                </div>
                <hr />
                <div class="modal-footer" style="margin-top: -20px;">
                    <button type="button" class="btn btn-primary" onclick="CreateOrUpdate()"><i class="glyphicon glyphicon-floppy-save"></i> Lưu lại</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="glyphicon glyphicon-minus-sign"></i> Thoát</button>
                </div>
            </div>
        </div>
    </div>
</div>
